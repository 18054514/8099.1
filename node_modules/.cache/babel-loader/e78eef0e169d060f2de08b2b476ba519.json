     <el-checkbox v-model=\"form.shadow\" @change=\"$emit('changeShadow', form.shadow)\">shadow</el-checkbox>\r\n            </el-form-item>\r\n          </el-row>\r\n          <el-form-item label=\"Line width\">\r\n            <el-input-number\r\n              v-model=\"form.strokeWidth\"\r\n              @change=\"$emit('changeStrokeWidth', form.strokeWidth)\"\r\n              :min=\"1\"\r\n              :max=\"10\"\r\n              label=\"Line width\"\r\n            ></el-input-number>\r\n          </el-form-item>\r\n          <el-form-item label=\"Line style\" v-if=\"!isNode\">\r\n            <el-select\r\n              class=\"select-edgetype\"\r\n              v-model=\"form.edgeStyle\"\r\n              @change=\"$emit('edgeChange',form.edgeStyle)\"\r\n              placeholder=\"Please select\"\r\n              round\r\n            >\r\n              <el-option\r\n                v-for=\"item in edgeOptions\"\r\n                :key=\"item.value\"\r\n                :label=\"item.label\"\r\n                :value=\"item.value\"\r\n              ></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item v-if=\"isNode\" label=\"Configuration serial number (reserved)\">\r\n            <el-input-number\r\n              v-model=\"newConfigOrder\"\r\n              @change=\"$emit('changeConfigOrder', {newConfigOrder})\"\r\n              :min=\"1\"\r\n              :max=\"10\"\r\n              label=\"配置序号\"\r\n            ></el-input-number>\r\n          </el-form-item>\r\n<!--          <el-form-item v-if=\"isNode&&!form.orderPoint\" label=\"Node picture\">-->\r\n<!--            <el-select-->\r\n<!--              @change=\"$emit('changeNodeimage', nodeImageUrl)\"-->\r\n<!--              v-model=\"nodeImageUrl\"-->\r\n<!--              filterable-->\r\n<!--              placeholder=\"Please select\"-->\r\n<!--            >-->\r\n<!--              <el-option-->\r\n<!--                v-for=\"item in nodeImageOptions\"-->\r\n<!--                :key=\"item.value\"-->\r\n<!--                :label=\"item.label\"-->\r\n<!--                :value=\"item.value\"-->\r\n<!--              >-->\r\n<!--                <el-tooltip class=\"item\" effect=\"dark\" :content=\"item.label\" placement=\"top\">-->\r\n<!--                  <div style=\"width:100%\">-->\r\n<!--                    <el-image class=\"select-image\" :src=\"item.value\">-->\r\n<!--                      <div slot=\"error\" class=\"image-slot\">-->\r\n<!--                        <i class=\"el-icon-picture-outline\"></i>-->\r\n<!--                      </div>-->\r\n<!--                    </el-image>-->\r\n<!--                  </div>-->\r\n<!--                </el-tooltip>-->\r\n<!--              </el-option>-->\r\n<!--            </el-select>-->\r\n<!--          </el-form-item>-->\r\n        </el-form>\r\n      </el-tab-pane>\r\n      <el-tab-pane label=\"text\">\r\n        <el-form :inline=\"true\" :model=\"form\" class=\"style-form\" size=\"mini\" label-position=\"left\">\r\n          <el-form-item label=\"Text content\">\r\n            <el-input v-model=\"cellTextValue\" @change=\"$emit('textValueChange', cellTextValue)\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"Font size\">\r\n            <el-input-number\r\n              v-model=\"form.fontSize\"\r\n              @change=\"$emit('changeFontSize', form.fontSize)\"\r\n            ></el-input-number>\r\n          </el-form-item>\r\n          <el-row>\r\n            <el-form-item>\r\n              <el-checkbox v-model=\"fontIsBold\">overbold</el-checkbox>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-checkbox v-model=\"fontIsIncline\">tilt</el-checkbox>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-checkbox v-model=\"fontIsUnderline\">underline</el-checkbox>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-checkbox v-model=\"fontIsStrickout\">Strikeout line</el-checkbox>\r\n            </el-form-item>\r\n          </el-row>\r\n          <el-form-item label=\"Font color\">\r\n            <el-color-picker\r\n              v-model=\"form.fontColor\"\r\n              show-alpha\r\n              :predefine=\"predefineColors\"\r\n              @change=\"$emit('changeFontColor', form.fontColor)\"\r\n            ></el-color-picker>\r\n          </el-form-item>\r\n          <el-form-item label=\"Text field background color\">\r\n            <el-color-picker\r\n              show-alpha\r\n              :predefine=\"predefineColors\"\r\n              v-model=\"form.labelBackgroundColor\"\r\n              @change=\"$emit('changeLabelBackgroundColor',form.labelBackgroundColor)\"\r\n            ></el-color-picker>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-tab-pane>\r\n    </el-tabs>\r\n    <!-- 鼠标坐标展示 -->\r\n    <div class=\"graphLocation\">\r\n      <span style=\"width:60px;display:inline-block;\">X:{{(graphX)}}</span>\r\n      <span style=\"width:60px;display:inline-block;\">Y:{{(graphY)}}</span>\r\n    </div>\r\n    <!-- 收缩小工具 -->\r\n    <div class=\"show-map\" ref=\"showMap\"></div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  props: ['isNode', 'cellStyle', 'graphY', 'graphX', 'currentNormalType', 'textValue'],\r\n  data () {\r\n    return {\r\n      edgeOptions: [\r\n        { label: \"Straight line\", value: 'SideToSide' },\r\n        { label: 'Broken line', value: \"orthogonalEdgeStyle\" }\r\n      ],\r\n      form: {\r\n        edgeStyle: \"折线\",\r\n        dashed: \"0\",\r\n        strokeColor: \"#6482b9\",\r\n        fontColor: \"#6482b9\",\r\n        strokeWidth: \"1\",\r\n        labelBackgroundColor: \"#FFFFFF\",\r\n        fontSize: 12,\r\n        fillColor: \"#FFFFFF\",\r\n        shadow: 0,\r\n        fontBold: 0,\r\n        fontStyle: 0\r\n      },\r\n      fontIsBold: false,\r\n      fontIsIncline: false,\r\n      fontIsUnderline: false,\r\n      fontIsStrick